{
  "name": "aionui-plugin-web-search",
  "version": "1.0.0",
  "description": "Example AionUi plugin that adds web search capabilities across all AI providers",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "aionui": {
    "pluginVersion": "1.0",
    "displayName": "Web Search",
    "description": "Search the web from any AI provider in AionUi. Results are injected as context into the conversation.",
    "icon": "./assets/icon.png",
    "category": "ai-tools",
    "capabilities": ["tools", "hooks", "adapters"],
    "minHostVersion": "1.7.0",
    "adapters": {
      "claude": "./dist/adapters/claude.js",
      "gemini": "./dist/adapters/gemini.js",
      "codex": "./dist/adapters/codex.js"
    },
    "permissions": [
      "network:fetch"
    ],
    "settings": {
      "searchEngine": {
        "type": "select",
        "label": "Search Engine",
        "description": "Which search engine to use for web queries",
        "default": "google",
        "options": [
          { "label": "Google", "value": "google" },
          { "label": "Bing", "value": "bing" },
          { "label": "DuckDuckGo", "value": "duckduckgo" }
        ]
      },
      "maxResults": {
        "type": "number",
        "label": "Max Results",
        "description": "Maximum number of search results to return",
        "default": 5,
        "validation": {
          "min": 1,
          "max": 20,
          "message": "Must be between 1 and 20"
        }
      },
      "apiKey": {
        "type": "string",
        "label": "API Key",
        "description": "Search API key (optional for DuckDuckGo)",
        "secret": true
      }
    }
  },
  "keywords": ["aionui", "plugin", "web-search"],
  "license": "MIT",
  "peerDependencies": {
    "@aionui/plugin-sdk": "^1.0.0"
  }
}
